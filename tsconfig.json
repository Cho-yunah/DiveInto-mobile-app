      "@legacy_redux/*": ["./src/legacy/lib/redux/*"],
      "@legacy_hooks/*": ["./src/legacy/lib/hooks/*"],

      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@navigators/*": ["./src/navigators/*"],
      "@recoil/*": ["./src/recoil/*"],
